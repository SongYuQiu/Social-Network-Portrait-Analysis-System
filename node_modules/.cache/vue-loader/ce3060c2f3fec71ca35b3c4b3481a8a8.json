               <activity />\n              </el-tab-pane>\n              <el-tab-pane label=\"Timeline\" name=\"timeline\">\n                <timeline />\n              </el-tab-pane>\n<!--              <el-tab-pane label=\"Account\" name=\"account\">-->\n<!--                <account :user=\"user\" />-->\n<!--              </el-tab-pane>-->\n            </el-tabs>\n          </el-card>\n        </el-col>\n\n      </el-row>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex'\nimport UserCard from './components/UserCard'\nimport Activity from './components/Activity'\nimport Timeline from './components/Timeline'\nimport Account from './components/Account'\nimport { getUserPortrait} from '@/api/weibo-user'\n\nexport default {\n  name: 'Profile',\n  components: { UserCard, Activity, Timeline, Account },\n  data() {\n    return {\n      user: {},\n      activeTab: 'activity',\n      weibo_user_id:''\n    }\n  },\n  computed: {\n    ...mapGetters([\n      'name',\n      'avatar',\n    ])\n  },\n  created() {\n    this.getUser()\n  },\n  methods: {\n    getUser() {\n      this.weibo_user_id = localStorage.getItem('weibo_user_id')\n      getUserPortrait(this.weibo_user_id).then(response => {\n        console.log(response.data)\n        this.user = {\n          name:response.data.screen_name,\n          avatar: response.data.avatar_hd,\n          weibo_user_id:response.data.weibo_user_id,\n          description:response.data.description !== \"\" ? response.data.description : \"暂无简介\",\n          verified:response.data.verified,\n          verified_reason:response.data.verified ? response.data.verified_reason : \"尚未认证\",\n          gender:response.data.gender,\n          interest:response.data.interest,\n          gender_probability:response.data.gender_probability,\n          eight_probability:response.data.eight_probability,\n          seven_probability:response.data.seven_probability,\n          nine_probability:response.data.nine_probability,\n          location:response.data.location,\n          member_rank:response.data.member_rank,\n          weibo_rank:response.data.weibo_rank,\n          since_date:response.data.since_date,\n          followers_count:response.data.followers_count,\n          follow_count:response.data.follow_count,\n          last_date:response.data.last_date,\n          portrait_date:response.data.portrait_date,\n          statuses_count:response.data.statuses_count\n        }\n      })\n\n    }\n  }\n}\n</script>\n"]}]}