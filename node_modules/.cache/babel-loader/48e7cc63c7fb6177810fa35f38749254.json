tTick(() => {\n      this.initChart()\n    })\n  },\n  beforeDestroy() {\n    if (!this.chart) {\n      return\n    }\n    this.chart.dispose()\n    this.chart = null\n  },\n  methods: {\n    initChart() {\n      this.chart = echarts.init(this.$el, 'macarons')\n\n      this.chart.setOption({\n        tooltip: {\n          trigger: 'item',\n          formatter: '{b}:{d}%'\n        },\n        legend: {\n          left: 'center',\n          bottom: '10',\n          data: ['male', 'female']\n        },\n        series: [\n          {\n            name: '性别预测',\n            type: 'pie',\n            roseType: 'radius',\n            radius: [15, 95],\n            center: ['50%', '50%'],\n            data: [\n              { value: 500, name: 'male' },\n              { value: 500, name: 'female' },\n              // { value: 149, name: 'Forex' },\n              // { value: 100, name: 'Gold' },\n              // { value: 59, name: 'Forecasts' }\n            ],\n            animationEasing: 'cubicInOut',\n            animationDuration: 2600\n          }\n        ]\n      })\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/dashboard/admin/components"}]}